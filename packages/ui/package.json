    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@types/lodash": "^4.14.182",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.25",
    "@vitejs/plugin-react": "^2.0.0",
    "concurrently": "^7.3.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-svg-loader": "^3.4.0"
