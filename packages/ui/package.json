{
  "name": "@toaonly-makina/ui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "dist/ui.umd.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/*.d.ts",
    "dist/*.js"
